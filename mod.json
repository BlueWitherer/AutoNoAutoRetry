{
	"geode": "3.1.1",
    "gd": {
        "android": "2.206",
        "win": "2.206",
        "mac": "2.206"
    },
	"version": "v1.0.0",
	"id": "hiimjustin000.auto_no_auto_retry",
	"name": "Auto No Auto Retry",
	"developer": "hiimjustin000",
	"description": "A mod that turns off auto-retry after a set percentage.",
    "repository": "https://github.com/hiimjustin000/AutoNoAutoRetry",
    "settings": {
        "enable": {
            "name": "Enable",
            "description": "Whether or not the mod is enabled.",
            "type": "bool",
            "default": true
        },
        "percentage": {
            "name": "Percentage",
            "description": "The percentage of the level that must be completed before auto-retry is turned off.",
            "type": "int",
            "default": 100,
            "min": 0,
            "max": 100
        }
    },
    "resources": {
        "sprites": [
            "resources/*.png"
        ]
    },
    "dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=1.12.0",
            "importance": "required"
        }
    ],
    "tags": [
        "customization",
        "enhancement",
        "gameplay",
        "offline"
    ]
}
